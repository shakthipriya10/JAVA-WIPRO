import java.util.Arrays;

public class FindExtremes {
    public static void main(String[] args) {
        int[] numbers = {3, 7, 1, 9, 12, 5, 8};
        Arrays.sort(numbers);
        System.out.println("sorted array");
        for (int i = 0; i < numbers.length; i++)   
        {       
        System.out.println(numbers[i]);   
        }   
        } 
}
    
